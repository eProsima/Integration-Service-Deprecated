<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="Text">
            <participants>
                <participant name="domain0"/>
                <participant name="domain5"/>
            </participants>
        </type>
    </topic_types>

    <profiles>
        <participant profile_name="domain0">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="domain5">
            <rtps>
                <builtin>
                    <domainId>5</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="is_subscriber">
            <topic>
                <name>TextPubSubTopic</name>
                <dataType>Text</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="is_publisher">
            <topic>
                <name>TextPubSubTopic</name>
                <dataType>Text</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="domain_change"> 
        <subscriber participant_name="domain0" subscriber_name="is_subscriber"/>
        <publisher participant_name="domain5" publisher_name="is_publisher"/>
    </connector>
</is>
