<?xml version="1.0"?>
<is>
    <is_types>
        <type name="HelloWorld">
            <library>build\Debug\keyhelloworld_static_static_lib.dll</library>
        </type>
        <type name="sample">
            <library>build\Debug\keyhelloworld_static_static_lib.dll</library>
        </type>
    </is_types>

    <profiles>
        <participant profile_name="HelloWorldParticipant">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subHW">
            <topic>
                <name>HelloWorldTopic</name>
                <dataType>HelloWorld</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="pubHW">
            <topic>
                <name>HelloWorldTopic</name>
                <dataType>HelloWorld</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <participant profile_name="KeysParticipant">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subK">
            <topic>
                <name>samplePubSubTopic</name>
                <dataType>sample</dataType>
                <kind>WITH_KEY</kind>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="pubK">
            <topic>
                <name>samplePubSubTopic</name>
                <dataType>sample</dataType>
                <kind>WITH_KEY</kind>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="HW_to_Key">
        <subscriber participant_profile="HelloWorldParticipant" subscriber_profile="subHW"/>
        <publisher participant_profile="KeysParticipant" publisher_profile="pubK"/>
        <transformation file="C:\git\integration-services\examples\dynamic_types\build\Debug\keyhelloworld_static_static_lib.dll" function="HelloWorldToKey"/>
    </connector>
