<?xml version="1.0"?>
<rs>
	<bridge>
        <bridge_type>ngsiv2</bridge_type>
		<ros2>
			<participant>ros2_participant</participant>
			<domain>0</domain>
			<topic>RobotTopic</topic>
			<type>RobotSnd</type>
            <partition>rt</partition>
		</ros2>
		<ngsiv2>
			<participant>ngsiv2_participant</participant>
			<id>.*</id>
            <host>localhost</host>
            <port>1026</port>
            <subscription>
                <type>Robot</type> <!-- optional -->
                <notifs>id,transmission_time,floor,x,y,zeta</notifs> <!-- comma separated -->
                <listener_host>localhost</listener_host>
                <listener_port>12345</listener_port>
            </subscription>
		</ngsiv2>
		<transformation>/home/luisgp/git/eProsima/routing-service/examples/TIS_NGSIv2/build/libuserlib.so</transformation>
	</bridge>
</rs>
