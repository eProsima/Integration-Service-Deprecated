<?xml version="1.0"?>
<is>
    <profiles>
        <participant profile_name="rtps">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>

            <subscriber profile_name="fastrtps_subscriber">
                <topic>
                    <name>HelloWorldTopic</name>
                    <dataType>HelloWorld</dataType>
                </topic>
                <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            </subscriber>
        </participant>
    </profiles>

    <bridge name="file">
        <library>C:/git/integration-services/examples/helloworld_to_file/build/debug/isfile.dll</library>

        <publisher name="file_publisher">
            <property>
                <name>filename</name>
                <value>output</value>
            </property>
            <property>
                <name>format</name>
                <value>txt</value>
            </property>
            <property>
                <name>append</name>
                <value>true</value>
            </property>
        </publisher>
    </bridge>

    <connector name="dump_to_file">
        <subscriber participant_name="rtps" subscriber_name="fastrtps_subscriber"/>
        <publisher participant_name="file" publisher_name="file_publisher"/>
    </connector>
</is>
