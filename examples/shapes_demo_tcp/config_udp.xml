<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="ShapeType">
            <library>libshapelib.so</library>
            <participants> <!-- Participants who will register it -->
                <participant name="LocalShapes"/>
                <participant name="TCPShapes"/>
            </participants>
        </type>
        <types_library>libshapelib.so</types_library> <!-- Only one needed in this case. Kept for testing. -->
    </topic_types>

    <profiles>
        <participant profile_name="LocalShapes">
            <rtps>
                <builtin>
                    <domainId>1</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>0</seconds>
                            <fraction>333</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="TCPShapes">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>0</seconds>
                            <fraction>333</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subscriberSquare">
            <topic>
                <name>Square</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <subscriber profile_name="subscriberTriangle">
            <topic>
                <name>Triangle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <subscriber profile_name="subscriberCircle">
            <topic>
                <name>Circle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="publisherSquare">
            <topic>
                <name>Square</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <publisher profile_name="publisherTriangle">
            <topic>
                <name>Triangle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <publisher profile_name="publisherCircle">
            <topic>
                <name>Circle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
                <ownership>
                    <kind>SHARED</kind>
                </ownership>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="Local_TCP_SQUARES">
        <subscriber participant_name="LocalShapes" subscriber_name="subscriberSquare"/>
        <publisher participant_name="TCPShapes" publisher_name="publisherSquare"/>
    </connector>

    <connector name="Local_TCP_TRIANGLES">
        <subscriber participant_name="LocalShapes" subscriber_name="subscriberTriangle"/>
        <publisher participant_name="TCPShapes" publisher_name="publisherTriangle"/>
    </connector>

    <connector name="Local_TCP_CIRCLES">
        <subscriber participant_name="LocalShapes" subscriber_name="subscriberCircle"/>
        <publisher participant_name="TCPShapes" publisher_name="publisherCircle"/>
    </connector>
