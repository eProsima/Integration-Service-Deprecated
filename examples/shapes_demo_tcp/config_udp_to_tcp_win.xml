<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="ShapeType">
            <library>libshapelib.dll</library>
        </type>
    </topic_types>

    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPv4_Server</transport_id>
                <type>TCPv4</type>
                <wait_negotiation>false</wait_negotiation>
                <listening_ports>
                    <port>5100</port>
                </listening_ports>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="LocalShapes">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="TCPShapes">
            <rtps>
                <userTransports>
                    <transport_id>TCPv4_Server</transport_id>
                </userTransports>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <subscriber profile_name="subscriberSquare">
            <topic>
                <name>Square</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <subscriber profile_name="subscriberTriangle">
            <topic>
                <name>Triangle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <subscriber profile_name="subscriberCircle">
            <topic>
                <name>Circle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>6</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="publisherSquare">
            <topic>
                <name>Square</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <publisher profile_name="publisherTriangle">
            <topic>
                <name>Triangle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <publisher profile_name="publisherCircle">
            <topic>
                <name>Circle</name>
                <dataType>ShapeType</dataType>
                <kind>WITH_KEY</kind>
                <historyQos>
                    <kind>KEEP_LAST</kind>
                    <depth>20</depth>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
                <liveliness>
                    <kind>AUTOMATIC</kind>
                    <lease_duration>INFINITE</lease_duration>
                </liveliness>
                <durability>
                    <kind>TRANSIENT_LOCAL</kind>
                </durability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="Local_TCP_SQUARES">
        <subscriber participant_profile="LocalShapes" subscriber_profile="subscriberSquare"/>
        <publisher participant_profile="TCPShapes" publisher_profile="publisherSquare"/>
    </connector>

    <connector name="Local_TCP_TRIANGLES">
        <subscriber participant_profile="LocalShapes" subscriber_profile="subscriberTriangle"/>
        <publisher participant_profile="TCPShapes" publisher_profile="publisherTriangle"/>
    </connector>

    <connector name="Local_TCP_CIRCLES">
        <subscriber participant_profile="LocalShapes" subscriber_profile="subscriberCircle"/>
        <publisher participant_profile="TCPShapes" publisher_profile="publisherCircle"/>
    </connector>
