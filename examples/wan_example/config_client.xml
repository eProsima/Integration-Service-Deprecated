<?xml version="1.0"?>
<is>
    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPv4_Client</transport_id>
                <type>TCPv4</type>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="LocalParticipant">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="TCPParticipant">
            <rtps>
                <userTransports>
                    <transport_id>TCPv4_Client</transport_id>
                </userTransports>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <sec>DURATION_INFINITY</sec>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <sec>3</sec>
                    </leaseAnnouncement>
                    <initialPeersList>
                        <locator>
                            <tcpv4>
                                <address>88.88.74.14</address>
                                <physical_port>5100</physical_port>
                            </tcpv4>
                        </locator>
                    </initialPeersList>
                </builtin>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <subscriber profile_name="HelloWorldSubscriber">
            <topic>
                <name>HelloWorldTopic</name>
                <dataType>HelloWorld</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

        <publisher profile_name="HelloWorldPublisher">
            <topic>
                <name>HelloWorldTopic</name>
                <dataType>HelloWorld</dataType>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="RemoteToLocal">
        <reader participant_profile="TCPParticipant" subscriber_profile="HelloWorldSubscriber"/>
        <writer participant_profile="LocalParticipant" publisher_profile="HelloWorldPublisher"/>
    </connector>
</is>
