<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="Data1mb">
            <participants>
                <participant name="ParticipantA"/>
                <participant name="ParticipantB1"/>
                <participant name="ParticipantB2"/>
                <participant name="ParticipantB3"/>
                <participant name="ParticipantB4"/>
                <participant name="ParticipantB5"/>
            </participants>
        </type>
    </topic_types>

    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPClient</transport_id>
                <type>TCPv4</type>
                <!--
                <listening_ports>
                    <port>5100</port>
                </listening_ports>
                <metadata_logical_port>0</metadata_logical_port>
                -->
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantA">
            <rtps>
                <userTransports>
                    <transport_id>TCPClient</transport_id>
                </userTransports>
                <defaultUnicastLocatorList>
                    <locator>
                        <kind>TCPv4</kind>
                        <address>127.0.0.1</address>
                        <ports_>
                            <physical_port>5100</physical_port>
                        </ports_>
                    </locator>
                </defaultUnicastLocatorList>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                    <metatrafficUnicastLocatorList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5100</physical_port>
                            </ports_>
                        </locator>
                    </metatrafficUnicastLocatorList>
                    <initialPeersList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5100</physical_port>
                            </ports_>
                        </locator>
                    </initialPeersList>
                </builtin>
                <use_IP4_to_send>true</use_IP4_to_send>
                <use_IP6_to_send>false</use_IP6_to_send>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB1">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN5_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB2">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN6_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB3">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN7_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB4">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN8_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB5">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN9_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subscriber">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </subscriber>

        <publisher profile_name="publisher">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </publisher>
    </profiles>

    <connector name="Data1mb_1_1_TCP_UDP_Static">
        <subscriber participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB1" publisher_profile="publisher"/>
    </connector>

    <connector name="Data1mb_1_2_TCP_UDP_Static">
        <subscriber participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB2" publisher_profile="publisher"/>
    </connector>

    <connector name="Data1mb_1_3_TCP_UDP_Static">
        <subscriber participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB3" publisher_profile="publisher"/>
    </connector>

    <connector name="Data1mb_1_4_TCP_UDP_Static">
        <subscriber participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB4" publisher_profile="publisher"/>
    </connector>

    <connector name="Data1mb_1_5_TCP_UDP_Static">
        <subscriber participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB5" publisher_profile="publisher"/>
    </connector>
</is>
