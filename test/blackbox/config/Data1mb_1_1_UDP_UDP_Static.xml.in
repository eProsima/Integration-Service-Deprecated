<?xml version="1.0"?>
<is>
    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>UDP</transport_id>
                <type>UDPv4</type>
                <maxInitialPeersRange>20</maxInitialPeersRange>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantA">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN5_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subscriber">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </subscriber>

        <publisher profile_name="publisher">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </publisher>
    </profiles>

    <connector name="Data1mb_1_1_UDP_UDP_Static">
        <reader participant_profile="ParticipantA" subscriber_profile="subscriber"/>
        <writer participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>
</is>
