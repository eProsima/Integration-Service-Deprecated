<?xml version="1.0"?>
<is>
    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPServerString</transport_id>
                <type>TCPv4</type>
                <listening_ports>
                    <port>5101</port>
                </listening_ports>
                <wait_negotiation>false</wait_negotiation>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantB_A1">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB_B">
            <rtps>
                <userTransports>
                    <transport_id>TCPServerString</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN5_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                </builtin>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <subscriber profile_name="subscriberB">
            <topic>
                <name>StringTopic</name>
                <dataType>StringType</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </subscriber>

        <publisher profile_name="publisherB">
            <topic>
                <name>StringTopic</name>
                <dataType>StringType</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </publisher>
    </profiles>

    <connector name="String_1_1_UDP_TCP_Static">
        <reader participant_profile="ParticipantB_A1" subscriber_profile="subscriberB"/>
        <writer participant_profile="ParticipantB_B" publisher_profile="publisherB"/>
    </connector>
</is>
