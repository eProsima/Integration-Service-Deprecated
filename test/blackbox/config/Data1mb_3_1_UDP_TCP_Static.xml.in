<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="Data1mb">
            <participants>
                <participant name="ParticipantA1"/>
                <participant name="ParticipantA2"/>
                <participant name="ParticipantA3"/>
                <participant name="ParticipantB"/>
            </participants>
        </type>
    </topic_types>

    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPServer</transport_id>
                <type>TCPv4</type>
                <listening_ports>
                    <port>5101</port>
                </listening_ports>
                <metadata_logical_port>7200</metadata_logical_port>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantA1">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantA2">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN1_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantA3">
            <rtps>
                <builtin>
                    <domainId>@DOMAIN2_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB">
            <rtps>
                <userTransports>
                    <transport_id>TCPServer</transport_id>
                </userTransports>
                <defaultUnicastLocatorList>
                    <locator>
                        <kind>TCPv4</kind>
                        <address>127.0.0.1</address>
                        <ports_>
                            <physical_port>5101</physical_port>
                        </ports_>
                    </locator>
                </defaultUnicastLocatorList>
                <builtin>
                    <domainId>@DOMAIN5_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                    <metatrafficUnicastLocatorList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5101</physical_port>
                            </ports_>
                        </locator>
                    </metatrafficUnicastLocatorList>
                </builtin>
                <use_IP4_to_send>true</use_IP4_to_send>
                <use_IP6_to_send>false</use_IP6_to_send>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <subscriber profile_name="subscriber">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </subscriber>

        <publisher profile_name="publisher">
            <topic>
                <name>Data1mbTopic</name>
                <dataType>Data1mb</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="String_1_1_UDP_UDP_Static">
        <subscriber participant_profile="ParticipantA1" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>

    <connector name="String_2_1_UDP_UDP_Static">
        <subscriber participant_profile="ParticipantA2" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>

    <connector name="String_3_1_UDP_UDP_Static">
        <subscriber participant_profile="ParticipantA3" subscriber_profile="subscriber"/>
        <publisher participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>
</is>
