<?xml version="1.0"?>
<is>
    <is_types>
        <type name="Keyed">
            <library>libTypes.so</library>
        </type>
    </is_types>

    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>TCPClient</transport_id>
                <type>TCPv4</type>
				<keep_alive_frequency_ms>100</keep_alive_frequency_ms>
				<keep_alive_timeout_ms>10000</keep_alive_timeout_ms>
                <wait_negotiation>false</wait_negotiation>
                <maxInitialPeersRange>20</maxInitialPeersRange>
            </transport_descriptor>
            <transport_descriptor>
                <transport_id>UDP</transport_id>
                <type>UDPv4</type>
                <maxInitialPeersRange>20</maxInitialPeersRange>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantA1">
            <rtps>
                <userTransports>
                    <transport_id>TCPClient</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>1</seconds>
                            <fraction>0</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                    <initialPeersList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>@PORT_RANDOM_NUMBER_5@</physical_port>
                            </ports_>
                        </locator>
                    </initialPeersList>
                </builtin>
                <useBuiltinTransports>false</useBuiltinTransports>
            </rtps>
        </participant>

        <participant profile_name="ParticipantA2">
