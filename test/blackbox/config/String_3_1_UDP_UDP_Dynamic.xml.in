<?xml version="1.0"?>
<is>
    <is_types>
        <type name="StringType">
            <library>libTypes.so</library>
        </type>
    </is_types>

    <profiles>
        <transport_descriptors>
            <transport_descriptor>
                <transport_id>UDP</transport_id>
                <type>UDPv4</type>
                <maxInitialPeersRange>20</maxInitialPeersRange>
            </transport_descriptor>
        </transport_descriptors>

        <participant profile_name="ParticipantA1">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN0_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantA2">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN1_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantA3">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN2_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <participant profile_name="ParticipantB">
            <rtps>
                <userTransports>
                    <transport_id>UDP</transport_id>
                </userTransports>
                <builtin>
                    <domainId>@DOMAIN5_RANDOM_NUMBER@</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                </builtin>
            </rtps>
        </participant>

        <subscriber profile_name="subscriber">
            <topic>
                <name>StringType</name>
                <dataType>StringType</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
        </subscriber>

        <publisher profile_name="publisher">
            <topic>
                <name>StringType</name>
                <dataType>StringType</dataType>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                </historyQos>
            </topic>
            <qos>
                <reliability>
                    <kind>RELIABLE</kind>
                </reliability>
            </qos>
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>
    </profiles>

    <connector name="String_1_1_UDP_UDP_Dynamic">
        <reader participant_profile="ParticipantA1" subscriber_profile="subscriber"/>
        <writer participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>

    <connector name="String_2_1_UDP_UDP_Dynamic">
        <reader participant_profile="ParticipantA2" subscriber_profile="subscriber"/>
        <writer participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>

    <connector name="String_3_1_UDP_UDP_Dynamic">
        <reader participant_profile="ParticipantA3" subscriber_profile="subscriber"/>
        <writer participant_profile="ParticipantB" publisher_profile="publisher"/>
    </connector>
</is>
